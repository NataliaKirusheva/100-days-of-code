// Sorted Union - Freecodecamp

function uniteUnique(...arr) {

const result = [];
result.push(arr[0][0]);

  for(let i = 0; i < arr.length; i++) {
    for(let j = 0; j < arr[i].length; j++) {
      
        if(!result.includes(arr[i][j])) {
          result.push(arr[i][j]);
          
        } 
      
    } 
  }

    
  return console.log(result);
}

uniteUnique([1, 3, 2], [5, 2, 1, 4], [2, 1]);
